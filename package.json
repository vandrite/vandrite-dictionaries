  "repository": "https://github.com/vandrite/vandrite-dictionaries",
  "bugs": "https://github.com/vandrite/vandrite-dictionaries/issues",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/wooorm"
  },
