    "Thomas Beverley <tom.beverley101@gmail.com>",
    "Vandrite <contact@vandrite.com> (https://vandrite.com)"
  ],
  "type": "module",
  "readme": "README.md",
  "devDependencies": {
    "@types/leb128": "^0.0.0",
    "@types/node": "^20.0.0",
    "@types/utf-8-validate": "^5.0.0",
    "bcp-47": "^2.0.0",
    "bcp-47-normalize": "^2.0.0",
    "is-hidden": "^2.0.0",
    "iso-15924": "^3.0.0",
    "iso-3166": "^4.0.0",
    "iso-639-3": "^3.0.0",
    "leb128": "^0.0.5",
    "mdast-zone": "^6.0.0",
    "prettier": "^3.0.0",
    "remark-cli": "^11.0.0",
    "remark-preset-wooorm": "^9.0.0",
    "type-coverage": "^2.0.0",
    "type-fest": "^4.0.0",
    "typescript": "^5.0.0",
    "utf-8-validate": "^6.0.0",
    "xo": "^0.56.0"
  },
  "scripts": {
    "build": "tsc --build --clean && tsc --build && type-coverage",
    "format": "remark . --frail --output --quiet && prettier . -w --log-level warn && xo --fix",
    "generate": "sh script/crawl.sh && node --conditions development script/generate.js",
    "generate:libreoffice": "node --conditions development script/crawl-libreoffice.js && node --conditions development script/generate.js",
    "test": "npm run build && npm run format && npm run test-api",
    "test-api": "node --conditions development test.js"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-wooorm",
      [
        "remark-gfm",
        {
          "tablePipeAlign": false
        }
      ],
      [
        "remark-lint-table-pipe-alignment",
        false
      ],
      "./script/list-of-dictionaries.js"
    ]
  },
  "typeCoverage": {
    "atLeast": 100,
    "detail": true,
    "ignoreCatch": true,
    "strict": true
  },
  "xo": {
    "overrides": [
      {
        "files": [
          "script/**/*.js",
          "test.js"
        ],
        "rules": {
          "no-await-in-loop": "off"
        }
      }
    ],
    "prettier": true,
    "rules": {
      "unicorn/prefer-string-replace-all": "off",
      "unicorn/prefer-switch": "off"
    }
  },
